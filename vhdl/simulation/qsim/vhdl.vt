	if (
		( binary_output_expected_prev[0] !== 1'bx ) && ( binary_output_prev[0] !== binary_output_expected_prev[0] )
		&& ((binary_output_expected_prev[0] !== last_binary_output_exp[0]) ||
			on_first_change[1])
	)
