reg [3:0] last_binary_outputp_exp;

reg trigger;

integer i;
integer nummismatches;

reg [1:2] on_first_change ;


initial
begin
trigger = 0;
i = 0;
nummismatches = 0;
on_first_change = 2'b1;
end

// update real /o prevs

always @(trigger)
begin
	binary_output_prev = binary_output;
	binary_outputp_prev = binary_outputp;
end

// update expected /o prevs

always @(trigger)
begin
	binary_output_expected_prev = binary_output_expected;
	binary_outputp_expected_prev = binary_outputp_expected;
end


