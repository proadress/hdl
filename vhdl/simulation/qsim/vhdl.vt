	$display("| expected binary_output = %b | expected binary_outputp = %b | ",binary_output_expected_prev,binary_outputp_expected_prev);
	$display("| real binary_output = %b | real binary_outputp = %b | ",binary_output_prev,binary_outputp_prev);
