module vhdl_vlg_vec_tst();
// constants                                           
// general purpose registers
reg clk;
reg [3:0] gray_input;
reg rst;
// wires                                               
wire [3:0] binary_output;
wire [3:0] binary_outputp;

wire sampler;                             

// assign statements (if any)                          
vhdl i1 (
// port map - connection between master ports and signals/registers   
	.binary_output(binary_output),
	.binary_outputp(binary_outputp),
	.clk(clk),
	.gray_input(gray_input),
	.rst(rst)
);

// clk
always
begin
	clk = 1'b0;
	clk = #10000 1'b1;
	#10000;
end 

// rst
initial
begin
	rst = 1'b0;
end 
// gray_input[ 3 ]
initial
begin
	gray_input[3] = 1'b0;
end 
// gray_input[ 2 ]
initial
begin
	gray_input[2] = 1'b1;
	gray_input[2] = #130000 1'b0;
end 
// gray_input[ 1 ]
initial
begin
	gray_input[1] = 1'b1;
	gray_input[1] = #130000 1'b0;
	gray_input[1] = #110000 1'b1;
	gray_input[1] = #60000 1'b0;
end 
// gray_input[ 0 ]
initial
begin
	gray_input[0] = 1'b0;
	gray_input[0] = #240000 1'b1;
	gray_input[0] = #60000 1'b0;
end 

vhdl_vlg_sample_tst tb_sample (
	.clk(clk),
	.gray_input(gray_input),
	.rst(rst),
	.sampler_tx(sampler)
);

vhdl_vlg_check_tst tb_out(
	.binary_output(binary_output),
	.binary_outputp(binary_outputp),
	.sampler_rx(sampler)
);
endmodule

