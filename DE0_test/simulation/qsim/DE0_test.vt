`timescale 1 ps/ 1 ps
module DE0_test_vlg_sample_tst(
	BOT,
	clk,
	SW,
	sampler_tx
);
input [2:0] BOT;
input  clk;
input [9:0] SW;
output sampler_tx;

reg sample;
time current_time;
always @(BOT or clk or SW)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

